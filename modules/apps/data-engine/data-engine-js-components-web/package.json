{
	"dependencies": {
		"@clayui/button": "3.32.0",
		"@clayui/data-provider": "3.35.3",
		"@clayui/drop-down": "3.35.3",
		"@clayui/icon": "3.32.0",
		"@clayui/layout": "3.32.0",
		"@clayui/loading-indicator": "3.32.0",
		"@clayui/pagination": "3.35.3",
		"@clayui/tabs": "3.35.3",
		"@clayui/tooltip": "3.35.3",
		"classnames": "2.3.1",
		"dom-align": "1.10.4",
		"frontend-js-web": "*",
		"incremental-dom-string": "0.0.2",
		"metal-component": "2.16.8",
		"metal-soy": "2.16.9",
