	@description = "This is a test for LRQA-68747. This test verifies that is possible to edit Label, Placeholder and Help text of fields that are inside the Fields Group."
	@priority = "5"
	test CanEditLabelPlaceholderAndHelpTextOfFields {
		NavItem.gotoStructures();

		WebContentStructures.addCP(structureName = "WC Structure Title");

		DEBuilder.addField(
			fieldLabel = "Text",
			fieldName = "Text");

		DEBuilder.addFieldNested(
			fieldLabel = "Numeric",
			fieldName = "Numeric",
			targetFieldLabel = "Text");

		DataEngine.editFieldTip(
			fieldFieldLabel = "Text",
			fieldHelp = "Help Text",
			fieldPlaceholder = "Placeholder");

		DataEngine.editFieldTip(
			fieldFieldLabel = "Numeric",
			fieldHelp = "Help Text",
			fieldPlaceholder = "Placeholder");

		WebContentStructures.saveCP();

		WebContentNavigator.gotoEditStructure(structureName = "WC Structure Title");

		DataEngine.editFieldLabel(
			fieldFieldLabel = "Text",
			fieldFieldLabelEdit = "Text Edited");

		DataEngine.editFieldLabel(
			fieldFieldLabel = "Numeric",
			fieldFieldLabelEdit = "Numeric Edited");

		DataEngine.editFieldTip(
			fieldFieldLabel = "Text Edited",
			fieldHelp = "Help Text Edited",
			fieldPlaceholder = "Placeholder Edited");

		DataEngine.editFieldTip(
			fieldFieldLabel = "Numeric Edited",
			fieldHelp = "Help Text Edited",
			fieldPlaceholder = "Placeholder Edited");

		WebContentStructures.saveCP();

		WebContentNavigator.gotoEditStructure(structureName = "WC Structure Title");

		DataEngine.viewField(
			fieldFieldLabel = "Text Edited",
			fieldHelp = "Help Text Edited",
			fieldName = "Text",
			fieldPlaceholder = "Placeholder Edited");

		DataEngine.viewField(
			fieldFieldLabel = "Numeric Edited",
			fieldHelp = "Help Text Edited",
			fieldName = "Numeric",
			fieldPlaceholder = "Placeholder Edited");
	}

