@component-name = "data-engine"
definition {

	property portal.release = "true";
	property portal.upstream = "true";
	property testray.main.component.name = "Data Engine";

	setUp {
		TestCase.setUpPortalInstance();

		User.firstLoginPG();

		JSONGroup.addGroup(groupName = "Test Site Name");

		WebContentNavigator.openWebContentAdmin(siteURLKey = "test-site-name");
	}

	tearDown {
		var testPortalInstance = PropsUtil.get("test.portal.instance");

		if ("${testPortalInstance}" == "true") {
			PortalInstances.tearDownCP();
		}
		else {
			JSONGroup.deleteGroupByName(groupName = "Test Site Name");
		}
	}

	@description = "This is a test for LRQA-68747. This test verifies that is possible to create Fields Group by adding a field from the sidebar into a field that is on the builder."
	@priority = "5"
	test CanBeCreated {
		NavItem.gotoStructures();

		WebContentStructures.addCP(structureName = "WC Structure Title");

		DEBuilder.addField(
			fieldLabel = "Text",
			fieldName = "Text");

		DataEngine.addFieldNested(
			fieldFieldLabel = "Numeric",
			fieldName = "Numeric",
			targetFieldLabel = "Text");

		WebContentStructures.saveCP();

		WebContentNavigator.gotoEditStructure(structureName = "WC Structure Title");

		DEBuilder.assertFieldsGroup(
			fieldGroupDepth = "1",
			fieldGroupLabel = "Fields Group",
			fieldsLabelList = "Text,Numeric");
	}

	@description = "This is a test for LRQA-68747. This test verifies that is possible to create Fields Group by dragging a field already on the builder into another field on the builder."
	@priority = "5"
	test CanBeCreatedWithFieldsOnBuilder {
		NavItem.gotoStructures();

		WebContentStructures.addCP(structureName = "WC Structure Title");

		DEBuilder.addField(
			fieldLabel = "Text",
			fieldName = "Text");

		DEBuilder.addField(
			fieldLabel = "Numeric",
			fieldName = "Numeric");

		DEBuilder.nestCreatedFields(
			fieldLabel = "Numeric",
			targetFieldLabel = "Text");

		WebContentStructures.saveCP();

		WebContentNavigator.gotoEditStructure(structureName = "WC Structure Title");

		DEBuilder.assertFieldsGroup(
			fieldGroupDepth = "1",
			fieldGroupLabel = "Fields Group",
			fieldsLabelList = "Text,Numeric");
	}

	@description = "This is a test for LRQA-68747. This test verifies that is possible to delete the entire Fields Group."
	@priority = "5"
	test CanBeDeleted {
		NavItem.gotoStructures();

		WebContentStructures.addCP(structureName = "WC Structure Title");

		DEBuilder.addField(
			fieldLabel = "Text",
			fieldName = "Text");

		DataEngine.addFieldNested(
			fieldFieldLabel = "Numeric",
			fieldName = "Numeric",
			targetFieldLabel = "Text");

		DEBuilder.addField(
			fieldLabel = "Date",
			fieldName = "Date");

		WebContentStructures.saveCP();

		WebContentNavigator.gotoEditStructure(structureName = "WC Structure Title");

		DataEngine.delete(fieldsGroupLabel = "Fields Group");

		WebContentStructures.saveCP();

		WebContentNavigator.gotoEditStructure(structureName = "WC Structure Title");

		DataEngine.assertFieldNotPresent(fieldsGroupLabel = "Fields Group");
	}

	@description = "This is a test for LRQA-68747. This test verifies that is possible to duplicate the entire Fields Group."
	@priority = "4"
	test CanBeDuplicated {
		NavItem.gotoStructures();

		WebContentStructures.addCP(structureName = "WC Structure Title");

		DEBuilder.addField(
			fieldLabel = "Text",
			fieldName = "Text");

		DataEngine.addFieldNested(
			fieldFieldLabel = "Numeric",
			fieldName = "Numeric",
			targetFieldLabel = "Text");

		DataEngine.duplicate(fieldsGroupLabel = "Fields Group");

		WebContentStructures.saveCP();

		WebContentNavigator.gotoEditStructure(structureName = "WC Structure Title");

		DEBuilder.assertFieldsGroup(
			fieldGroupDepth = "1",
			fieldGroupLabel = "Fields Group",
			fieldsLabelList = "Text,Numeric");

		DEBuilder.assertFieldsGroup(
			fieldGroupDepth = "1",
			fieldGroupLabel = "Copy of Fields Group",
			fieldsLabelList = "Copy of Text,Copy of Numeric");
	}

	@description = "This is a test for LRQA-68747. This test verifies that is possible to delete a field inside a Fields Group."
	@priority = "5"
	test CanDeleteField {
		NavItem.gotoStructures();

		WebContentStructures.addCP(structureName = "WC Structure Title");

		DEBuilder.addField(
			fieldLabel = "Text",
			fieldName = "Text");

		DataEngine.addFieldNested(
			fieldFieldLabel = "Numeric",
			fieldName = "Numeric",
			targetFieldLabel = "Text");

		WebContentStructures.saveCP();

		WebContentNavigator.gotoEditStructure(structureName = "WC Structure Title");

		DataEngine.delete(fieldFieldLabel = "Numeric");

		WebContentStructures.saveCP();

		WebContentNavigator.gotoEditStructure(structureName = "WC Structure Title");

		DataEngine.assertFieldNotPresent(fieldFieldLabel = "Numeric");

		DEBuilder.assertFieldsGroup(
			fieldGroupDepth = "1",
			fieldGroupLabel = "Fields Group",
			fieldsLabelList = "Text");
	}

	@description = "This is a test for LRQA-68747. This test verifies that is possible to duplicate the inner Fields Group that is inside another Fields Group."
	@priority = "3"
	test CanDuplicateNestedFieldsGroup {
		NavItem.gotoStructures();

		WebContentStructures.addCP(structureName = "WC Structure Title");

		DEBuilder.addField(
			fieldLabel = "Text",
			fieldName = "Text");

		DEBuilder.addFieldNested(
			fieldLabel = "Numeric",
			fieldName = "Numeric",
			targetFieldLabel = "Text");

		DataEngine.editFieldsGroupLabel(
			fieldsGroupLabel = "Fields Group",
			fieldsGroupLabelEdit = "Fields Group 1");

		DEBuilder.addFieldNested(
			fieldLabel = "Date",
			fieldName = "Date",
			targetFieldLabel = "Numeric");

		DataEngine.editFieldsGroupLabel(
			fieldsGroupLabel = "Fields Group",
			fieldsGroupLabelEdit = "Fields Group 2");

		DataEngine.duplicate(fieldsGroupLabel = "Fields Group 2");

		WebContentStructures.saveCP();

		WebContentNavigator.gotoEditStructure(structureName = "WC Structure Title");

		DEBuilder.assertFieldsGroup(
			fieldGroupDepth = "2",
			fieldGroupLabel = "Fields Group 2",
			fieldsLabelList = "Numeric,Date");

		DEBuilder.assertFieldsGroup(
			fieldGroupDepth = "2",
			fieldGroupLabel = "Copy of Fields Group 2",
			fieldsLabelList = "Copy of Numeric,Copy of Date");
	}

	@description = "This is a test for LRQA-68747. This test verifies that is possible to duplicate or delete fields that are inside a Fields Group."
	@priority = "5"
