	@description = "Verify users without edit permission cannot change the currency of a channel."
	@priority = "3"
	test CanNotEditChannelCurrencyAsUserWithoutEditPermission {
		property custom.properties = "jsonws.web.service.paths.excludes=${line.separator}company.security.strangers.verify=false";
		property portal.acceptance = "false";

		task ("Given a Commerce account with a buyer user") {
			CommerceEntry.addAccountEntryUser(
				accountName = "Commerce Account",
				accountType = "Business",
				agreeToTermsAndAnswerReminderQuery = "true",
				createNewAccount = "true",
				requireReset = "false",
				userEmailAddress = "buyer@liferay.com",
				userFirstName = "Buyer",
				userLastName = "User",
				userRole = "Buyer",
				userScreenName = "buyeruser",
				userSiteMembership = "Minium");
		}

		task ("And given the buyer has permission to view a channel's configuration") {
			CommercePermissions.defineChannelPermissionsViaJSONAPI(
				roleTitle = "User",
				update = "true",
				view = "true");
		}

		task ("When the buyer logs in and goes to the Minium channel") {
			User.logoutPG();

			CommerceLogin.miniumLogin(
				password = "test",
				urlAppend = "web/minium",
				userEmailAddress = "buyer@liferay.com");

			CommerceChannels.openChannelsAdmin();

			CommerceNavigator.gotoEntry(entryName = "Minium Portal");
		}

		task ("Then the buyer is unable to change the channel currency code") {
			AssertElementNotPresent(
				key_title = "Currency",
				locator1 = "CommerceEntry#CATALOGS_FORM");
		}
	}

	@description = "Test for COMMERCE-9432 | Can paginate Payment and Shipping Restrictions pages"
	@priority = "3"
	test CanPaginatePaymentAndShippingRestrictions {
		property test.name.skip.portal.instance = "CPCommerceChannels#CanPaginatePaymentAndShippingRestrictions";

		task ("Given the admin navigates to 'Minium Portal' channel") {
			CommerceChannels.openChannelsAdmin();

			CommerceNavigator.gotoEntry(entryName = "Minium Portal");
		}

		task ("When the admin navigates to the Payment/Shipping Restrictions tab") {
			for (var tab : list "Payment,Shipping") {
				CommerceEntry.gotoMenuTab(menuTab = "${tab} Restrictions");

				task ("Then the admin can use pagination to change the list of countries shown on the page") {
					Pagination.changePagination(itemsPerPage = "4 items");

					CommerceEntry.viewPaginationResults(results = "Showing 1 to 4 of 247");

					AssertElementNotPresent(
						key_rowNumber = "5",
						locator1 = "CommerceEntry#ENTRY_CONTENT_ROW_NUMBER");

					for (var country : list "Afghanistan,Aland Islands,Albania,Algeria") {
						AssertElementPresent(
							key_tableEntry = "${country}",
							locator1 = "LexiconTable#TABLE_ENTRY");
					}

					Click(locator1 = "Pagination#NEXT_LINK");

					CommerceEntry.viewPaginationResults(results = "Showing 5 to 8 of 247");

					AssertElementNotPresent(
						key_rowNumber = "5",
						locator1 = "CommerceEntry#ENTRY_CONTENT_ROW_NUMBER");

					for (var country : list "Afghanistan,Aland Islands,Albania,Algeria") {
						AssertElementNotPresent(
							key_tableEntry = "${country}",
							locator1 = "LexiconTable#TABLE_ENTRY");
					}

					Click(locator1 = "Pagination#PREVIOUS_LINK");

					AssertElementNotPresent(
						key_rowNumber = "5",
						locator1 = "CommerceEntry#ENTRY_CONTENT_ROW_NUMBER");

					for (var country : list "Afghanistan,Aland Islands,Albania,Algeria") {
						AssertElementPresent(
							key_tableEntry = "${country}",
							locator1 = "LexiconTable#TABLE_ENTRY");
					}

					Click(
						key_pageNumber = "4",
						locator1 = "CommerceNavigation#PAGINATION_ANY_PAGE");

					for (var country : list "Afghanistan,Aland Islands,Albania,Algeria") {
						AssertElementNotPresent(
							key_tableEntry = "${country}",
							locator1 = "LexiconTable#TABLE_ENTRY");
					}
				}
			}
		}
	}

	@description = "Verify users can set and edit delivery term eligibility for shipping options."
	@priority = "3"
	test CanSetAndEditDeliveryTermsEligibility {
		task ("Given an active delivery term") {
			CommerceJSONTermsAndConditionsAPI._addCommerceTermsAndConditions(
				termsCount = "2",
				termsType = "Delivery");
		}

		task ("When the user sets a shipping option's eligibility to a specific delivery term") {
			CommerceChannels.openChannelsAdmin();

			CommerceNavigator.gotoEntry(entryName = "Minium Portal");

			CommerceEntry.setEntryEligibility(
				deliveryTermsEligibility = "Delivery Term 1",
				shippingMethod = "Flat Rate",
				shippingOption = "Standard Delivery");
		}

		task ("Then the delivery term should be shown under Delivery Terms in the eligibility tab") {
			CommerceNavigator.gotoEntry(entryName = "Standard Delivery");

			SelectFrame.selectFrameNoLoading(locator1 = "CommerceEntry#IFRAME_SIDE_PANEL");

			Click(
				key_panel = "Details",
				locator1 = "Panel#PANEL_COLLAPSED");

			CommerceEntry.gotoMenuTab(menuTab = "Eligibility");

