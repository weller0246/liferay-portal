		AssertElementNotPresent(
			key_submissionName = "Blogs Entry 1",
			locator1 = "WorkflowSubmissions#SUBMISSION_BY_NAME");

		Click(locator1 = "WorkflowSubmissions#SUBMISSION_CLEAR_SEARCH_BUTTON");

