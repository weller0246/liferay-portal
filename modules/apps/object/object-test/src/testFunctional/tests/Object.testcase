		ObjectAdmin.publishObjectViaAPI(objectName = "CustomObject290");

		ObjectAdmin.openObjectAdmin();

		Click(
			key_objectLabel = "Custom Object 290",
			locator1 = "ObjectAdmin#OBJECT_KEBAB_MENU");

