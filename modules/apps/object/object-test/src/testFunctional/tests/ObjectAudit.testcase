	@description = "LPS-149508 - Verify if there are default resources of the System Objects on the Audit Portlet"
	@priority = "3"
	test CanViewDefaultResourcesOnSystemObjects {
		for (var loops : list "1,2,3,4,5,6") {
			Audit.openAuditAdmin();

			WaitForElementPresent(locator1 = "TextInput#SITE_SEARCH_KEYWORDS");

			Type(
				locator1 = "TextInput#SITE_SEARCH_KEYWORDS",
				value1 = "ObjectDefinition");

			Click(locator1 = "Button#SUBMIT");

			Audit.viewAuditDetailsByPosition(
				additionalInformation = "company,true",
				clientIp = "",
				key_resourceAction = "ADD",
				key_resourceName = "ObjectDefinition",
				nameByPosition = "${loops}",
				resourceAction = "ADD",
				resourceName = "ObjectDefinition",
				userName = "");

			if (("${sytemsObjects}" == "Account") || ("${sytemsObjects}" == "Commerce Order") || ("${sytemsObjects}" == "Commerce Product") || ("${sytemsObjects}" == "Commerce Product Group") || ("${sytemsObjects}" == "Postal Address") || ("${sytemsObjects}" == "User")) {
				AssertTextEquals.assertPartialText(
					locator1 = "AuditReports#REPORTS_DETAIL_ADDITIONAL_INFORMATION",
					value1 = "${sytemsObjects}");
			}
		}
	}

