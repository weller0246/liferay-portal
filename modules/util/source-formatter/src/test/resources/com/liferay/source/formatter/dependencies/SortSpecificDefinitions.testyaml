components:
    schemas:
        CollectionType:
            default: "all"
            enum: [active, all, production, recent]
            type: string
        ExternalLink:
            description: Represents a link to an entity in a different domain.
            properties:
                domain:
                    description: The external domain.
                    type: string
                dateCreated:
                    description: The external link's creation date.
                    format: date-time
                    readOnly: true
                    type: string
info:
    title: "Change Tracking REST"
    version: v1.0
openapi: 3.0.1
paths:
    "/projects/{projectKey}/contacts/{contactKey}/roles":
        delete:
            parameters:
                - in: path
                  name: contactKey
                  required: true
                  schema:
                      type: string
                - in: path
                  name: projectKey
                  required: true
                  schema:
                      type: string
                - in: query
                  name: contactRoleKeys
                  required: true
                  schema:
                      items:
                          type: string
                      type: array
            responses:
                204:
                    content:
                        application/json: {}
            tags: ["Project"]
        put:
            parameters:
                - in: path
                  name: contactKey
                  required: false
                  schema:
                      type: string
                - in: path
                  name: projectKey
                  required: false
                  schema:
                      type: string
                - in: query
                  name: contactRoleKeys
                  required: true
                  schema:
                      items:
                          type: string
                      type: array
            responses:
                204:
                    content:
                        application/json: {}
            tags: ["Project"]
    "/sites/{siteId}/structured-contents/by-key/{key}":
        get:
            operationId: getCollectionEntriesPage
            parameters:
                - in: header
                  name: Accept-Language
                  schema:
                      type: string
                - explode: true
                  in: query
                  name: changeTypesFilter
                  required: false
                  schema:
                      items:
                          type: string
                      type: array
                  style: form
                - explode: true
                  in: query
                  name: classNameIdsFilter
                  required: false
                  schema:
                      items:
                          type: string
                      type: array
                  style: form
                - explode: true
                  in: query
                  name: groupIdsFilter
                  required: false
                  schema:
                      items:
                          type: string
                      type: array
                  style: form
                - explode: true
                  in: query
                  name: userIdsFilter
                  required: false
                  schema:
                      items:
                          type: string
                      type: array
                  style: form
                - in: path
                  name: key
                  required: true
                  schema:
                      type: string
                - in: query
                  name: collision
                  required: false
                  schema:
                      default: false
                      type: boolean
                  style: form
                - in: query
                  name: pageSize
                  required: false
                  schema:
                      type: integer
                - in: query
                  name: page
                  required: false
                  schema:
                      type: integer
                - in: cookie
                  name: debug
                  schema:
                      type: integer
                      enum: [0, 1]
                      default: 0
                - in: cookie
                  name: csrftoken
                  schema:
                      type: string
                - in: query
                  name: sort
                  required: false
                  schema:
                      type: string
                - in: query
                  name: status
                  required: false
                  schema:
                      default: 2
                      type: integer
                - in: path
                  name: siteId
                  required: true
                  schema:
                      format: int64
                      type: integer
            responses:
                200:
                    content:
                        application/json:
                            schema:
                                items:
                                    $ref: "#/components/schemas/Entry"
                                type: array
                        application/xml:
                            schema:
                                items:
                                    $ref: "#/components/schemas/Entry"
                                type: array
            tags: ["Entry"]

---

tasks:
    # Install Git
    - name: Install Git
      package:
          state: present
          name: git
    # Deploy SSH key
    - name: Deploy SSH key
      copy:
          content: "{{ id_rsa_continuous-integration }}"
          dest: /home/rancher/.ssh/id_rsa_continuous-integration@liferay.com
          mode: 384
          owner: rancher
    - copy:
          content: |-
              {{ github_repos | sort | join(' ') }}
          dest: "/data/slaves/{{ item.hostname }}/git/liferay/.repositories"