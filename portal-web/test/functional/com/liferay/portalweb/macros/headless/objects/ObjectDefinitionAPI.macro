		var objectDefinitionStatus = ObjectDefinitionAPI._getObjectDefinitionStatusById(objectDefinitionId = "${objectDefinitionId}");

		TestUtils.assertEquals(
			actual = "${objectDefinitionStatus}",
			expected = "true");

		return "${objectDefinitionId}";
	}

