definition {

	macro addCSSURLRow {
		Click(
			key_id = "cssURLs",
			locator1 = "RemoteAppsEntry#ADD_URL_ROW");
	}

	macro addCustomElement {
		Type(
			key_text = "Name",
			locator1 = "TextInput#ANY",
			value1 = "${entryName}");

		Type(
			key_text = "HTML Element Name",
			locator1 = "TextInput#ANY",
			value1 = "${entryHtmlName}");

		Type(
			key_id = "ClientExtensionAdminPortlet_url",
			locator1 = "RemoteAppsEntry#URL",
			value1 = "${entryURL}");
	}

	macro addCustomElementAllFields {
		Type(
			key_text = "Name",
			locator1 = "TextInput#ANY",
			value1 = "${entryName}");

		Type(
			key_text = "HTML Element Name",
			locator1 = "TextInput#ANY",
			value1 = "${entryHtmlName}");

		Type(
			key_id = "ClientExtensionAdminPortlet_url",
			locator1 = "RemoteAppsEntry#URL",
			value1 = "${entryURL}");

		Type(
			key_text = "CSS URL",
			locator1 = "TextInput#ANY",
			value1 = "${entryCssurl}");

		Type(
			locator1 = "RemoteAppsEntry#REMOTE_APPS_PROPERTIES",
			value1 = "${entryProperties}");
	}

	macro addCustomElementMultipleURLs {
		Type(
			key_text = "Name",
			locator1 = "TextInput#ANY",
			value1 = "${entryName}");

		Type(
			key_text = "HTML Element Name",
			locator1 = "TextInput#ANY",
			value1 = "${entryHtmlName}");

		RemoteAppsEntry.addURLRow();

		RemoteAppsEntry.addCSSURLRow();

		Type(
			key_id = "ClientExtensionAdminPortlet_url",
			key_index = "1",
			locator1 = "RemoteAppsEntry#URL_ROW_INDEX",
			value1 = "${entryUrl1}");

		Type(
			key_id = "ClientExtensionAdminPortlet_url",
			key_index = "2",
			locator1 = "RemoteAppsEntry#URL_ROW_INDEX",
			value1 = "${entryUrl2}");

		Type(
			key_id = "cssURLs",
			key_index = "1",
			locator1 = "RemoteAppsEntry#URL_ROW_INDEX",
			value1 = "${entryCssurl1}");

		Type(
			key_id = "cssURLs",
			key_index = "2",
			locator1 = "RemoteAppsEntry#URL_ROW_INDEX",
			value1 = "${entryCssurl2}");
	}

	macro addEntry {
		Type(
			key_text = "Name",
			locator1 = "TextInput#ANY",
			value1 = "${entryName}");

		if (isSet(mainURL)) {
			Type(
				key_id = "ClientExtensionAdminPortlet_mainURL",
				locator1 = "RemoteAppsEntry#URL",
				value1 = "${mainURL}");
		}
		else {
			Type(
				key_id = "ClientExtensionAdminPortlet_url",
				locator1 = "RemoteAppsEntry#URL",
				value1 = "${entryURL}");
		}
	}

	macro addPortletCategory {
		Select(
			locator1 = "RemoteAppsEntry#REMOTE_APPS_PORTLET_CATEGORY_NAME",
			value1 = "${categoryName}");
	}

	macro addURLRow {
		Click(
			key_id = "ClientExtensionAdminPortlet__urls",
			locator1 = "RemoteAppsEntry#ADD_URL_ROW");
	}

	macro removeURLRow {
		Click(
			key_id = "${key_id}",
			locator1 = "RemoteAppsEntry#REMOVE_URL_ROW");
	}

}