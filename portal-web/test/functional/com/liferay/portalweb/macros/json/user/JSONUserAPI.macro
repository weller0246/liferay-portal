		}

		var curl = '''
			${portalURL}/api/jsonws/user/get-company-users \
			-u ${userName}:test \
			-d companyId=${companyId} \
			-d <CURL_DATA[start=-1]CURL_DATA> \
			-d <CURL_DATA[end=-1]CURL_DATA>
		''';

		var userIds = JSONCurlUtil.post("${curl}", "$..['userId']");

		return "${userIds}";
	}

